            <div class="sub-item">

              
             <div class="row">

              <form action="/leav"  id="add-form" method="POST">
                <div class="form-group">
                    <label for="exampleInputPassword1">Select Leave Type :</label>
                   
                      <input class="form-check-input" type="radio" value="casual leave" name="leave_type" id="flexRadioDefault1">Casual Leave
                     <input class="form-check-input" type="radio" value="Medical Leave Test" name="leave_type"  id="flexRadioDefault2" > Medical Leave Test
